{
  "name": "GHReader",
  "description": "GitHub Feed Reader",
  "version": "0.0.0",
  "dependencies": {
    "dacho": "^2.0.1",
    "react": "~15.3.2",
    "react-native": "^0.37.0",
    "react-redux": "^4.4.5",
    "redux": "^3.6.0",
    "redux-promise": "^0.5.3",
    "redux-thunk": "^2.1.0"
  },
  "devDependencies": {
    "babel-cli": "^6.18.0",
    "babel-core": "^6.18.2",
    "babel-eslint": "7.1.0",
    "babel-jest": "^17.0.0",
    "babel-plugin-espower": "^2.3.1",
    "babel-preset-react-native": "^1.9.0",
    "chokidar-cli": "^1.2.0",
    "clear-require": "^1.0.1",
    "codecov": "^1.0.1",
    "documentation": "^4.0.0-beta12",
    "eslint": "^3.9.1",
    "eslint-config-moqada": "^18.0.0",
    "eslint-plugin-flowtype": "^2.25.0",
    "eslint-plugin-jsdoc": "^2.3.1",
    "eslint-plugin-react": "^6.6.0",
    "eslint-plugin-react-native": "^2.0.0",
    "flow-bin": "^0.33.0",
    "isparta": "^4.0.0",
    "jest": "^17.0.0",
    "jest-react-native": "^17.0.0",
    "mocha": "^3.1.2",
    "npm-run-all": "^3.1.1",
    "nyc": "^9.0.1",
    "power-assert": "^1.4.1",
    "proxyquire": "^1.7.10",
    "react-addons-test-utils": "~15.3.2",
    "react-shallow-testutils": "^2.0.0",
    "react-test-renderer": "~15.3.2",
    "sinon": "^2.0.0-pre.4"
  },
  "jest": {
    "preset": "jest-react-native"
  },
  "private": true,
  "repository": {
    "type": "git",
    "url": "https://github.com/moqada/GHReader.git"
  },
  "scripts": {
    "clean": "rm -rf coverage",
    "codecov": "codecov",
    "doc": "documentation build src/**/*.js --document-exported --github --format html --output doc",
    "packager": "./node_modules/react-native/packager/packager.sh",
    "predoc": "rm -rf doc",
    "pretest": "rm -rf coverage",
    "start": "npm-run-all --parallel packager watch:*",
    "stop": "npm-run-all stop:*",
    "stop:flow": "flow stop",
    "test": "npm-run-all test:*",
    "test:coverage": "babel-node $(npm bin)/isparta cover --report text --report html --report lcovonly _mocha -- test/**/*spec.js",
    "test:flow": "flow",
    "test:jest": "jest",
    "test:lint": "eslint --cache {src,test}",
    "watch": "npm-run-all --parallel watch:*",
    "watch:flow": "flow & chokidar src/ -c flow",
    "watch:lint": "chokidar src/ test/ -c 'npm run -s test:lint'",
    "watch:test": "mocha --watch test/**/*spec.js"
  }
}
