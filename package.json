{
  "name": "GHReader",
  "description": "GitHub Feed Reader for iOS",
  "version": "0.0.0",
  "dependencies": {
    "dacho": "^2.0.1",
    "react-native": "^0.21.0",
    "react-redux": "^4.4.0",
    "redux": "^3.3.1",
    "redux-promise": "^0.5.1",
    "redux-thunk": "^2.0.1"
  },
  "devDependencies": {
    "babel-cli": "^6.6.5",
    "babel-core": "^6.7.2",
    "babel-eslint": "6.0.0-beta.3",
    "babel-plugin-espower": "^2.1.2",
    "babel-preset-es2015": "^6.6.0",
    "babel-preset-react": "^6.5.0",
    "chokidar-cli": "^1.2.0",
    "clear-require": "^1.0.1",
    "codecov.io": "^0.1.6",
    "esdoc": "^0.4.6",
    "esdoc-es7-plugin": "0.0.3",
    "eslint": "^2.4.0",
    "eslint-config-moqada": "^12.0.1",
    "eslint-plugin-react": "^4.2.1",
    "eslint-plugin-react-native": "^1.0.0",
    "flow-bin": "0.21.0",
    "isparta": "^4.0.0",
    "mocha": "^2.4.5",
    "npm-run-all": "^1.5.2",
    "power-assert": "^1.3.1",
    "proxyquire": "^1.7.4",
    "react": "^0.14.7",
    "react-addons-test-utils": "^0.14.7",
    "react-shallow-testutils": "^1.0.0",
    "sinon": "^1.17.3"
  },
  "private": true,
  "repository": {
    "type": "git",
    "url": "https://github.com/moqada/GHReader.git"
  },
  "scripts": {
    "clean": "rm -rf coverage",
    "codecov": "cat coverage/lcov.info | codecov",
    "doc": "esdoc -c esdoc.json",
    "lint": "eslint --cache {src,test}",
    "packager": "./node_modules/react-native/packager/packager.sh",
    "postinstall": "XARGS_NO_RUN=`[ \\`uname\\` = 'Linux' ] && echo '--no-run-if-empty' || echo ''` && find ./node_modules -name .babelrc | grep -v react-native | grep -v /babel/ | xargs $XARGS_NO_RUN rm",
    "predoc": "rm -rf doc",
    "pretest": "rm -rf coverage",
    "start": "npm-run-all --parallel packager watch:*",
    "stop": "npm-run-all stop:*",
    "stop:flow": "flow stop",
    "test": "npm-run-all lint test:*",
    "test:coverage": "babel-node $(npm bin)/isparta cover --report text --report html --report lcovonly _mocha -- test/**/*spec.js",
    "test:flow": "flow",
    "watch": "npm-run-all --parallel watch:*",
    "watch:flow": "flow & chokidar src/ -c flow",
    "watch:lint": "chokidar src/ test/ -c 'npm run -s lint'",
    "watch:test": "mocha --watch test/**/*spec.js"
  }
}
